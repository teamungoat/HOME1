\n","    else:\n","        unclassified.append(item)\n","\n","# 중복 제거하고 정렬\n","classified = list(dict.fromkeys(classified))\n","\n","# 정렬된 리스트 생성\n","sorted_list = [category for category in desired_order if category in classified]\n","sorted_list.extend(unclassified)\n","\n","print(sorted_list)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"DJCyzYtVGi8C","executionInfo":{"status":"ok","timestamp":1723275366088,"user_tz":-540,"elapsed":9,"user":{"displayName":"크복짜","userId":"12410456823605642737"}},"outputId":"1a3d13c3-defb-4b7b-903b-3e2dc2c8f892"},"execution_count":26,"outputs":[{"output_type":"stream","name":"stdout","text":["['액션/서부극', '드라마', 'SF/무협/판타지/어드벤처', '범죄/수사/추리/스릴러', '멜로/로맨스', '코미디', '애니메이션', '사극/시대극', '공포', '다큐멘터리', '기타']\n"]}]},{"cell_type":"code","source":["genre_favor=pd.read_excel('/content/연령별 장르 선호도.xlsx')\n","genre_favor.drop(index=0, inplace=True)\n","genre_favor.columns"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"G2MXJJ97N08l","executionInfo":{"status":"ok","timestamp":1723275366974,"user_tz":-540,"elapsed":891,"user":{"displayName":"크복짜","userId":"12410456823605642737"}},"outputId":"76414b53-1999-4784-c020-7aa3f131ce43"},"execution_count":27,"outputs":[{"output_type":"execute_result","data":{"text/plain":["Index([' ', 'Unnamed: 1', '사례수', '액션', 'SF/판타지/무협', '드라마', '로맨틱코미디',\n","       '범죄/수사/추리/스릴러', '사극/시대극', '멜로/로맨스', '코미디', '애니메이션', '다큐멘터리', '공포',\n","       '실험/예술영화', '성인/에로', '기타', '합계'],\n","      dtype='object')"]},"metadata":{},"execution_count":27}]},{"cell_type":"code","source":["genre_favor.drop(columns=[' '], inplace=True)"],"metadata":{"id":"MWbBQrjbTw1U","executionInfo":{"status":"ok","timestamp":1723275366974,"user_tz":-540,"elapsed":29,"user":{"displayName":"크복짜","userId":"12410456823605642737"}}},"execution_count":28,"outputs":[]},{"cell_type":"code","source":["genre_favor.columns\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"IjnGC4iVTTEU","executionInfo":{"statu